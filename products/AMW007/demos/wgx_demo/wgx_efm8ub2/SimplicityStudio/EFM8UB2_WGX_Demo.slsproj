  <module id="com.silabs.sdk.si8051.external.efm8PeripheralDriver.wdt0"/>
  <module id="com.silabs.sdk.si8051.external.efm8Library.assert"/>
  <module id="com.silabs.sdk.si8051.external.bsp.efm8_joystick"/>
  <module id="com.silabs.sdk.si8051.external.bsp.efm8_memory_lcd"/>
  <module id="com.silabs.sdk.si8051.external.bsp.efm8_rgb_led"/>
  <module id="com.silabs.sdk.si8051.external.bsp.efm8_bsp"/>  
