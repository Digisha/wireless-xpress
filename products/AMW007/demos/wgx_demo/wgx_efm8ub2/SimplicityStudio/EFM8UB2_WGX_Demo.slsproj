      <file name="memory_lcd_config.h"/>
      <file name="oscilloscope_config.h"/>
      <file name="rgb_led_config.h"/>
      </folder>
  </folder>
  <folder includeAllFiles="true" includeAllFolders="true" name="inc" uri="inc"/>
  <folder includeAllFiles="true" includeAllFolders="true" name="ui" uri="../common/ui"/>
  <folder includeAllFiles="true" includeAllFolders="true" name="wgx" uri="../../../lib/wgx"/>
  <folder includeAllFiles="true" includeAllFolders="true" name="src" uri="src"/>
